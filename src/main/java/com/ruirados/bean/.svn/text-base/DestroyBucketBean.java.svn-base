public class DestroyBucketBean {
	private String bucketName;
	private String companyId;
	private String zoneId;
	
	public DestroyBucketBean(){}
	
	/**
	 * @param bucketName
	 * @param companyId
	 * @param zoneId
	 */
	public DestroyBucketBean(String bucketName, String companyId, String zoneId) {
		super();
		this.bucketName = bucketName;
		this.companyId = companyId;
		this.zoneId = zoneId;
	}
	/**
	 * Getter method for property <tt>bucketName</tt>.
	 * 
	 * @return property value of bucketName
	 */
	public String getBucketName() {
		return bucketName;
	}
	/**
	 * Setter method for property <tt>bucketName</tt>.
	 * 
	 * @param bucketName value to be assigned to property bucketName
	 */
	public void setBucketName(String bucketName) {
		this.bucketName = bucketName;
	}
	/**
	 * Getter method for property <tt>companyId</tt>.
	 * 
	 * @return property value of companyId
	 */
	public String getCompanyId() {
		return companyId;
	}
	/**
	 * Setter method for property <tt>companyId</tt>.
	 * 
	 * @param companyId value to be assigned to property companyId
	 */
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	/**
	 * Getter method for property <tt>zoneId</tt>.
	 * 
	 * @return property value of zoneId
	 */
	public String getZoneId() {
		return zoneId;
	}
	/**
	 * Setter method for property <tt>zoneId</tt>.
	 * 
	 * @param zoneId value to be assigned to property zoneId
	 */
	public void setZoneId(String zoneId) {
		this.zoneId = zoneId;
	}
	
}
