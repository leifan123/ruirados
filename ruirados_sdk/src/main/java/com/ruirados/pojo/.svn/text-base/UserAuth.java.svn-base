package com.ruirados.pojo;

import java.util.Date;

import java.io.Serializable;

public class UserAuth  implements Serializable{

	private static final long serialVersionUID = 1526261070522884023L;
	private Integer id;

	public UserAuth setId(Integer id){
		 this.id = id;
		 return this;
	}

	public Integer getId(){
		 return id;
	}
	private String companyid;

	public UserAuth setCompanyid(String companyid){
		 this.companyid = companyid;
		 return this;
	}

	public String getCompanyid(){
		 return companyid;
	}
	private String username;

	public UserAuth setUsername(String username){
		 this.username = username;
		 return this;
	}

	public String getUsername(){
		 return username;
	}
	private String sfcode;

	public UserAuth setSfcode(String sfcode){
		 this.sfcode = sfcode;
		 return this;
	}

	public String getSfcode(){
		 return sfcode;
	}
	private String tel;

	public UserAuth setTel(String tel){
		 this.tel = tel;
		 return this;
	}

	public String getTel(){
		 return tel;
	}
	private String data;

	public UserAuth setData(String data){
		 this.data = data;
		 return this;
	}

	public String getData(){
		 return data;
	}
	private Date createtime;

	public UserAuth setCreatetime(Date createtime){
		 this.createtime = createtime;
		 return this;
	}

	public Date getCreatetime(){
		 return createtime;
	}
}